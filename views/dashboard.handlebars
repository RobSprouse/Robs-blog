<h1>Dashboard</h1>
<h2>Logged in as: {{user.username}}</h2>
<h3>My Blogs</h3>
<a href="/newBlog">Create a New Blog</a>
{{#each blogs}}
     <div class="blog">
          <h2>{{this.title}}</h2>
          <p>{{this.content}}</p>
          <p>Posted by: {{this.user.username}}</p>
          <p>Date: {{this.date_created}}</p>
     </div>
{{/each}}