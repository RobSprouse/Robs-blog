<div class="col-md-6">
     <h2>Signup</h2>
     <h4 id="logInErr" style="display: none;"></h4>
     <form id="formSignUp">
          <div class="formGroup">
               <label for="username">name:</label>
               <input class="formInput" type="text" id="username" />

          </div>
          <div class="formGroup">
               <label for="emailSignUp">email:</label>
               <input class="formInput" type="text" id="emailSignUp" />
          </div>
          <div class="formGroup">
               <label for="passwordSignUp">password:</label>
               <input class="formInput" type="password" id="passwordSignUp" />
          </div>
          <div class="formGroup">
               <button class="btn btn-primary" type="submit">Sign Up</button>
          </div>
     </form>
</div>
<script src="./js/signup.js"></script>